{
  "manifest_version": 3,
  "name": "YouTube Variable Skipper",
  "version": "1.0",
  "description": "Set custom skip time for YouTube videos",
  "permissions": ["storage"],
  "host_permissions": ["https://www.youtube.com/*"],
  "action": {
    "default_popup": "popup.html"
  },

  
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
